                &print_one_indiv_user_cpu_ratio_tab("Rstall_IU_use", "Instr_cnt" ,@pnum[0]);
                &print_one_indiv_user_cpu_ratio_tab("Rstall_storeQ", "Instr_cnt",@pnum [0]);
                &print_one_indiv_user_cpu_ratio_tab("Re_RAW_miss", "Instr_cnt",@pnum[0]);
                printf "(%d)\n",@pnum[0];
        }

	printf "\n";
}

